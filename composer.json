{
    "name": "genial-framework/session",
    "type": "library",
    "description": "A simple, fast, and secure session component built for web development!",
    "homepage": "https://genial.tech/php/genial-framework/components/session",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nicholas English",
            "email": "nenglish0820@outlook.com",
            "homepage": "https://genial.tech/php/genial-framework/authors/nenglish",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "genial-framework/env": "1.*",
        "genial-framework/helper-class": "1.*"
    },
    "autoload": {
        "psr-0": {
            "Genial": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "suggest": {
        "genial-framework/cache": "1.*",
        "genial-framework/mongodb": "1.*",
        "genial-framework/database": "1.*",
        "genial-framework/cyptography": "1.*"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
